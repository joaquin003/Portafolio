RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.*)$ $1.php   
Options All -Indexes
ErrorDocument 403 https://joaquin003.github.io/Portafolio/errores/pag403.html
ErrorDocument 404 https://joaquin003.github.io/Portafolio/errores/pag404.html
ErrorDocument 500 https://joaquin003.github.io/Portafolio/errores/pag500.html